<?xml version="1.0" encoding="UTF-8"?>
<payment-processors xmlns="http://www.demandware.com/xml/impex/paymentprocessor/2007-03-31">
    <payment-processor processor-id="Adyen">
        <description>redirect to Adyen payment page</description>
        <preferences>
            <all-instances/>
			   <development>
				<preference preference-id="Adyen_Debug">true</preference>
				<preference preference-id="Adyen_HMACkey">yourHMAC</preference>
				<preference preference-id="Adyen_PaymentSelection">one</preference>
				<preference preference-id="Adyen_merchantCode">yourMerchantAccount</preference>
				<preference preference-id="Adyen_skinCode">yourSkinCode</preference>
			   </development> 
            <staging/>
            <production/>
        </preferences>
    </payment-processor>
	<payment-processor processor-id="ADYEN_CREDIT">
		 <description>adyen credit card payment procesor</description>
        <preferences>
            <all-instances/>
            <development/>
            <staging/>
            <production/>
        </preferences>
    </payment-processor>
</payment-processors>
